{
  "name": "correos-mexico-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "build:backend": "cd apps/backend && pnpm run build",
    "start:backend:prod": "cd apps/backend && pnpm run start:prod"
  },
  "devDependencies": {
    "metro-runtime": "^0.83.3",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "base64-js": "^1.5.1"
  }
}
